#include <iostream>

using namespace std;

int main()
{
	int choice = 0;

	cout << "Pick any finger from Number 1 to Number 5" << endl;
	cout << "Your choice: ";
	cin >> choice;

	switch (choice) {

	case 1: 
		cout << "Your choices don't matter. You have no free will" << endl;
		break;

	case 2:
		cout << "Your choices don't matter. You have no free will" << endl;
			break;

	case 3:
		cout << "Your choices don't matter. You have no free will" << endl;
			break;

	case 4:
		cout << "Your choices don't matter. You have no free will" << endl;
			break;

	case 5:
		cout << "Your choices don't matter. You have no free will" << endl;
			break;

	default:
		cout << "Your choices don't matter. You have no free will" << endl;
			break;

	}

}
